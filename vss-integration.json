{
 "manifestVersion": 1,
 "id": "js-refactor",
 "version": "0.1.4",
 "name": "JS Refactor",
 "publisher": "cmstead",
 "description": "Javascript refactoring extension for Visual Studio Code.",
 "targets": [
     {
         "id": "Microsoft.VisualStudio.Services.Integration"
     }
 ],    
 "icons": {
     "default": "images/js-refactor-icon.png"
 },
