{

    "Use Strict": {
        "prefix": "strict",
        "body": [
            "'use strict';\n"
        ],
        "description": "Insert strict mode declaration"
    },

    "Member Function": {
        "prefix": "mfn",
        "body": [
            "${1:name}: function (${2:parameters}) {\n${3:body}\n}\n"
        ],
        "description": "Insert a member function into your prototype object"
    },

    "Function": {
        "prefix": "fn",
        "body": [
            "function ${1:name} (${2:parameters}){\n\t${3:body}\n}"
        ],
        "description": "Insert a new function"
    },

    "Anonymous Function": {
        "prefix": "anon",
        "body": [
            "function (${1:parameters}) {\t${2:body}}"
        ],
        "description": "Insert a new anonymous function"
    },

    "Immediately Invoked Function Expression": {
        "prefix": "iife",
        "body": [
            "(function (${1:parameters}) {",
            "${2:body}",
            "})(${3:arguments);"
        ],
        "description": "Insert a new immediately invoked function expression"
    }
}