{
    "cond": ["", 
             "{indent}if () {",
             "{body}",
             "{indent}}",
             ""],

    "function": ["",
                 "{indent}function {name}() {",
                 "{body}",
                 "{indent}}",
                 ""],

    "iife": ["",
             "{indent}(function () {",
             "{body}",
             "{indent}})();",
             ""],
             
