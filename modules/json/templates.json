                     ""],
    
    "addExport": {
        "single": "\nmodule.exports.{functionName} = {functionName}",
        "newExport": "\nmodule.exports = {\n\t{functionName}: {functionName}\n}",
        "objectAddition": "\n\t{functionName}: {functionName},"
    }
}