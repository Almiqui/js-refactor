{
    "cond": ["", 
             "{indent}if () {",
             "{body}",
             "{indent}}",
             ""],

    "function": ["",
                 "{indent}function {name}() {",
                 "{body}",
                 "{indent}}",
                 ""],

    "iife": ["",
             "{indent}(function () {",
             "{body}",
             "{indent}})();",
             ""],
             
    "functionCall": ["{indent}{name}();",
                     ""],

    "newVariable": ["{indent}var {name} = {value};",
                    ""],
    
    "addExport": {
        "single": ["", 
                   "module.exports.{functionName} = {functionName}"],
                   
        "newExport": ["",
                      "module.exports = {",
                      "\t{functionName}: {functionName}",
                      "}"],
                      
        "objectAddition": ["",
                           "\t{functionName}: {functionName},"]
    }
}